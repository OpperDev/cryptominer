{
  "name": "cryptominer",
  "productName": "Cryptominer",
  "version": "1.0.0",
  "description": "Simple cryptominer in nodejs",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --electron-version=5.0.1 --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . cryptominer --electron-version=5.0.1 --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Cryptominer\"",
    "package-linux": "electron-packager . cryptominer --electron-version=5.0.1 --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpperDev/cryptominer.git"
  },
  "author": "OpperDev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpperDev/cryptominer/issues"
  },
  "homepage": "https://github.com/OpperDev/cryptominer#readme",
  "dependencies": {
    "coin-imp": "^1.0.6",
    "electron": "^5.0.1",
    "electron-packager": "^13.1.1",
    "path": "^0.12.7",
    "jquery": "^3.4.1"
  }
}
